{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vue/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*"
  ],
  "modes": {
    "window": {
      "title": "neutralino-vue-starter",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/vue/public/favicon.ico"
    }
  },
  "cli": {
    "binaryName": "neutralino-vue-starter",
    "resourcesPath": "/vue/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/vue/public/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0",
    "frontendLibrary": {
        "patchFile": "/vue/index.html",
        "devUrl": "http://localhost:3000"
    }
  }
}