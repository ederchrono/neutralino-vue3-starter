{
  "name": "Neutralino-vue-starter",
  "version": "1.0.0",
  "description": "Starter template for Neutralino.js projects with Vue3 and Vite",
  "scripts": {
    "setup": "cd neutralino && neu update",
    "install:all": "pnpm recursive install && pnpm run setup",
    "dev": "cd neutralino/vue && pnpm run dev",
    "desktop": "cd neutralino && neu run --frontend-lib-dev -- --window-enable-inspector",
    "build-vue": "cd neutralino/vue && pnpm run build",
    "build": "pnpm run build-vue && cd neutralino && neu build --release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ederchrono/neutralino-vue-starter.git"
  },
  "keywords": [
    "Generators",
    "Templates"
  ],
  "author": "Eder DÃ­az",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ederchrono/neutralino-vue-starter/issues"
  },
  "homepage": "https://github.com/ederchrono/neutralino-vue-starter#readme",
  "devDependencies": {
    "@neutralinojs/neu": "^9.3.1"
  }
}
